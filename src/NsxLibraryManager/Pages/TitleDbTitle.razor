                    Publisher:
                </RadzenText>
            </RadzenColumn>
            <RadzenColumn Size="6" Style="display: inline-flex; align-items: center;">
                <RadzenText Text="@(LibraryTitleDto?.Publisher.NullStringToEmpty())" TextStyle="TextStyle.Body1" Style="margin-bottom: 0"/>
            </RadzenColumn>
        </RadzenRow>
        <RadzenRow Style="padding-bottom: 5px">
            <RadzenColumn Style="display: inline-flex; align-items: center;">
                <span class="rz-pr-2">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="28" role="presentation" alt=""
