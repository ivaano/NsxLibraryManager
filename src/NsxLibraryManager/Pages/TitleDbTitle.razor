@page "/TitleDbTitle/{titleId}"
@using NsxLibraryManager.Extensions

<PageTitle>Title From DbTitle</PageTitle>
<RadzenRow>
    <RadzenColumn Size="12" SizeMD="6">
        <RadzenImage Path="@(LibraryTitleDto?.BannerUrl.NullImageToEmpty())"
                     Style="width: 100%; border-radius: 20px;"></RadzenImage>
    </RadzenColumn>
    <RadzenColumn Size="12" SizeMD="6">
        <RadzenRow Style="padding-bottom: 5px" Gap="3px">
            <RadzenColumn>
