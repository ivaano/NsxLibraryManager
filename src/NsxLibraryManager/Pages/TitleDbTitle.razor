                </RadzenText>
            </RadzenColumn>
            <RadzenColumn Size="6" Style="display: inline-flex; align-items: center;">
                <RadzenText Text="@(LibraryTitleDto?.Rating.AgeRating(AgeRatingAgency))" TextStyle="TextStyle.Body1" Style="margin-bottom: 0"/>
